  "$id": "http://our.company.com/jobUpdate.schema.json",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "minSalary": {
      "type": "string",
      "pattern": "^[0-9]*$"
    },
    "hasEquity": {
      "type": "string",
      "enum": [
        "t",
        "true",
        "True",
        "TRUE",
        "T",
        "f",
        "false",
        "False",
        "FALSE",
        "F"
      ]
    }
  },
  "additionalProperties": false,
  "required": []
}